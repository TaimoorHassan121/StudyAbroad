@section Script{

    <script src="~/study-abroad/js/sessionstorage.js"></script>
    <script>
        let session = new Session();
        let filter = {
            course: "",
            university: ""
            //countryId: 0,
            //school:0
        }

        let courseInput = document.getElementById('courseId');
        let courses = document.getElementById('course-list');

        for (let option of courses.options) {
            option.onclick = function () {
                filter.course = courseInput.value = option.value;
                // filter.courseId = option.getAttribute('data-courseId');
                session.set('filter', filter);
                // console.log(filter)
                courses.style.display = 'none';
                courseInput.style.borderRadius = "5px";
            };
        };
        courseInput.onfocus = function () {
            if (universities.style.display == 'block')
                universities.style.display = 'none';
            courses.style.display = 'block';
            courses.style.maxWidth = courseInput.offsetWidth + "px";
            courseInput.style.borderRadius = "5px 5px 0 0";
        };

        courseInput.oninput = function () {
            currentCourseFocus = -1;
            var text = courseInput.value.toUpperCase();
            for (let option of courses.options) {
                if (option.value.toUpperCase().indexOf(text) > -1) {
                    option.style.display = "block";
                } else {
                    option.style.display = "none";
                }
            };
        }

        var currentCourseFocus = -1;
        courseInput.onkeydown = function (e) {
            if (e.keyCode == 40) {
                currentCourseFocus++
                addActiveCourse(courses.options);
            }
            else if (e.keyCode == 38) {
                currentCourseFocus--
                addActiveCourse(courses.options);
            }
            else if (e.keyCode == 13) {
                e.preventDefault();
                if (currentCourseFocus > -1) {
                    /*and simulate a click on the "active" item:*/
                    if (courses.options) courses.options[currentCourseFocus].click();
                }
            }
        }

        function addActiveCourse(x) {
            if (!x) return false;
            removeActive(x);
            if (currentCourseFocus >= x.length) currentCourseFocus = 0;
            if (currentCourseFocus < 0) currentCourseFocus = (x.length - 1);
            x[currentCourseFocus].classList.add("active");
        }


        let uniInput = document.getElementById('universityId');
        let universities = document.getElementById('university-list');
        for (let option of universities.options) {
            option.onclick = function () {
                filter.university = uniInput.value = option.value;

                //filter.universityId = option.getAttribute('data-uniId');

                session.set('filter', filter);
                universities.style.display = 'none';
                uniInput.style.borderRadius = "5px";
            }
        };
        uniInput.onfocus = function () {
            if (courses.style.display == 'block')
                courses.style.display = 'none';
            universities.style.display = 'block';
            universities.style.maxWidth = uniInput.offsetWidth + "px";
            universities.style.borderRadius = "5px 5px 0 0";
        };
        uniInput.oninput = function () {
            currentUniFocus = -1;
            var text = uniInput.value.toUpperCase();
            for (let option of universities.options) {
                if (option.value.toUpperCase().indexOf(text) > -1) {
                    option.style.display = "block";
                } else {
                    option.style.display = "none";
                }
            };
        }

        var currentUniFocus = -1;
        uniInput.onkeydown = function (e) {
            if (e.keyCode == 40) {
                currentUniFocus++
                addActiveUni(universities.options);
            }
            else if (e.keyCode == 38) {
                currentUniFocus--
                addActiveUni(universities.options);
            }
            else if (e.keyCode == 13) {
                e.preventDefault();
                if (currentUniFocus > -1) {
                    /*and simulate a click on the "active" item:*/
                    if (universities.options) universities.options[currentUniFocus].click();
                }
            }
        }
        function addActiveUni(x) {
            if (!x) return false;
            removeActive(x);
            if (currentUniFocus >= x.length) currentUniFocus = 0;
            if (currentUniFocus < 0) currentUniFocus = (x.length - 1);
            x[currentUniFocus].classList.add("active");
        }
        function removeActive(x) {
            for (var i = 0; i < x.length; i++) {
                x[i].classList.remove("active");
            }
        }

        let courseDiv = document.getElementById('course-div');
        let universityDiv = document.getElementById('university-div');
        document.addEventListener('click', function (event) {
            if (courseDiv !== event.target && !courseDiv.contains(event.target) && universityDiv !== event.target && !universityDiv.contains(event.target)) {
                //console.log('clicking outside the div');
                if (courses.style.display == 'block')
                    courses.style.display = 'none';
                else if (universities.style.display == 'block')
                    universities.style.display = 'none';
            }
        });

        function onSearch() {
              var studentProfile = @Html.Raw(Json.Serialize(Model.studentProfile.StudentId));
            if (session.get("filter"))
                filte = session.get("filter");
            var url = "/StudentProfiles/Search?id=" + studentProfile;

            if (courseInput.value)
                url = url + (!url.includes("/StudentProfiles/Search?id=" + studentProfile) ? "?" : "&") + "course=" + courseInput.value;
                //url = url + "?course=" + courseInput.value;

            if (uniInput.value)
                //url = url  + "?university=" + uniInput.value;
                url = url + (!url.includes("/StudentProfiles/Search?id=" + studentProfile) ? "?" : "&") + "university=" + uniInput.value;

            //if (filter.countryId)
            //    url = url + (!url.includes("/StudentProfiles/Search?") ? "?" : "&") + "countryId=" + filter.countryId;
            
            filter.course = courseInput.value;
            filter.university = uniInput.value;
            session.set('filter', filter);
            window.location.href = url;
            //console.log(url)
        }

        $(document).ready(function () {
            var obj = session.get("filter");
