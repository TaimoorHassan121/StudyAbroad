<div class="container-fluid bg-search">
    <div class="container">
        <div class="row">
            <div class="col-md-12 serch-program">
                <h1>Search Programs <br> & Schools</h1>
            </div>
        </div>
    </div>
</div>


<!-- input type  -->
<div class="container-fluid mt-5">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-4 p-0">
                        <div class="input-group input-search mb-3 " id="course-div">
                            <input type="text" autocomplete="off" role="combobox" list="" id="courseId" name="courseId" class="form-control input data-list-input" placeholder="What would you like to study?" aria-label="Recipient's username" aria-describedby="button-addon2">
                            <datalist id="course-list" role="listbox">
                                @if (ViewBag.Courses != null)
                                    foreach (var item in ViewBag.Courses)
                                    {
                                        <option value="@item.CourseName" data-courseId="@item.CourseId">@item.CourseName</option>
                                    }
                            </datalist>
                        </div>
                    </div>
                    <div class="col-md-8 p-0">
                        <div class="input-group input-search mb-3" id="university-div">
                            <input type="text" autocomplete="off" role="combobox" list="" id="universityId" name="universityId" class="form-control" placeholder="Where? e.g. school name or location" aria-label="Recipient's username" aria-describedby="button-addon2">
                            <datalist id="university-list" role="listbox">
                                @if (ViewBag.Universities != null)
                                    foreach (var item in ViewBag.Universities)
                                    {
                                        <option value="@item.UniversityName" data-uniId="@item.UniversityId">@item.UniversityName</option>
                                    }
                            </datalist>
                            <button class="btn btn-primary" type="button" id="button-addon2" onclick="onSearch()">
                                <span class="material-symbols-outlined">
                                    search
                                </span> &nbsp;&nbsp;  Search
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

