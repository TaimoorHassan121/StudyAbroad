@model IEnumerable<Study_Abroad.Models.Country>
@using Study_Abroad.Data;
@inject StudyAbroadContext _db;
@{
    ViewData["Title"] = "Home Page";
    Layout = "_LayoutStudyAbroad";
    var countries = _db.Countries.Where(a => a.Status).ToList();
    var courses =  _db.Courses.Where(a=>a.Status).ToList();

}
@section CSS{ 
    <style>
        datalist {
            position: absolute;
            background-color: white;
            border: 1px solid #86b7fe;
            border-radius: 0 0 5px 5px;
            border-top: none;
            font-family: sans-serif;
            /*width: 350px;*/
            top: 80px;
            padding: 5px;
            max-height: 10rem;
            overflow-y: auto;
            z-index: 5;
            border-color: #86b7fe;
            box-shadow: 0 0 0 0.25rem rgb(13 110 253 / 25%);
        }

        option {
            background-color: white;
            padding: 4px;
            /*color: blue;*/
            margin-bottom: 1px;
            font-size: 18px;
            cursor: pointer;
        }
    </style>
}
<!-- Start your Journey to Study Abroad START -->
<div class="container-fluid journey-sec mt-5">
    <div class="container">
        <div class="row">
            <div class="col-md-6" style="margin-top: 10%;">
                <h1>Start your Journey <br> to Study Abroad</h1>
                <p class="mt-4">
                    Vestibulum commodo sapien non elit porttitor, vitae volutpat nibh mollis. Nulla porta risus id neque tempor, in efficitur justo imperdiet. Etiam a ex at ante tincidunt imperdiet. Nunc congue ex vel nisl viverra, sit amet aliquet lectus
                    ullamcorper. Praesent luctus lacus non lorem
                </p>
                <div class="input-group mt-4">

                    <input type="text" autocomplete="off" role="combobox" list="" id="courseId" name="courseId" class="form-control" aria-label="Text input with segmented dropdown button" placeholder="Search for college or university">
                    <datalist id="course-list" role="listbox">
                        @foreach (var item in courses)
                            {
                        <option value="@item.CourseName" data-courseId="@item.CourseId">@item.CourseName</option>
                            }
                    </datalist>
                    <!-- <button type="button" class="btn dropdown-toggle dropdown-toggle-split" data-bs-toggle="dropdown" aria-expanded="false">
        USA
        </button>
    <ul class="dropdown-menu dropdown-menu-end">
